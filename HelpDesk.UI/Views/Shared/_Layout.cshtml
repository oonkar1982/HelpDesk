<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - HelpDesk.UI</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700;900&display=swap" rel="stylesheet">
    <link href="~/css/responsive.css" rel="stylesheet" />
   
    <link href="~/lib/datatables/css/jquery.datatables.css" rel="stylesheet" />
    <link href="~/lib/datatables/css/datatables.bootstrap4.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/HelpDesk.UI.styles.css" asp-append-version="true" />
    
</head>
<body>
   
    <div id="wrapper">
    <header>
            <nav class="navbar navbar-expand-sm navbar-dark">
                <a class="navbar-brand" href="#">
                    <img id="logo" src="https://i.imgur.com/K7Nwq4V.jpg"> Acme Inc
                    </a>
                <div class="collapse navbar-collapse">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link active"  asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Incidents" asp-action="Index">Incidents</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Customers" asp-action="Index">Customers</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-area="" asp-controller="Agents" asp-action="Index">Agent</a>
                        </li>
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
                
            </nav>
        
    
    </header>
   <main class="container-fluid min-vh-80">
            <partial name="_BusyIndicatorPartial" />
            @RenderBody()
            
    </main>
        <footer>

            <div class="bg-black py-2">
                <div class="container text-center">
                    <p class="text-muted mb-0 py-2">© 2019 Onkar All rights reserved.</p>
                </div>
            </div>
        </footer>

        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/datatables/js/jquery.datatables.min.js"></script>
        <script src="~/lib/datatables/js/datatables.bootstrap4.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    </div>
    @RenderSection("Scripts", required: false)
</body>
</html>
